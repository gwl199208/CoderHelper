        /// 
        /// </summary>
        [Key]
        public int singleContestId { get; set; }
        /// <summary>
        /// 
        /// </summary>
        [Required]
        [Column(TypeName = "nvarchar")]
        [StringLength(200)]
        public string singleContestName { get; set; }
        /// <summary>
        /// 
        /// </summary>
        [Required]
        [Column(TypeName = "datetime")]
        public DateTime singleContestBegintime { get; set; }
        /// <summary>
        /// 
        /// </summary>
        [Required]
        [Column(TypeName = "datetime")]
        public DateTime singleContestTime { get; set; }
        /// <summary>
        /// 
        /// </summary>
        [Required]
        public int singleContestPersonNum { get; set; }
        /// <summary>
        /// 
        /// </summary>
        [Required]
        public string singleContestDescription { get; set; }
        /// <summary>
        /// 
        /// </summary>
        [Required]
        [Column(TypeName = "nvarchar")]
        [StringLength(10)]
        public string singleContestLevel { get; set; }

        /// <summary>
        /// 
        /// </summary>
        public virtual UserModel singleContestLeader { get; set; }
        /// <summary>
        /// 
        /// </summary>
        public virtual ContestPermissionModel contestPermission { get; set; }
